{
  "name": "@holz/json-backend",
  "version": "0.8.2",
  "description": "Print logs as newline-delimited JSON.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/PsychoLlama/holz",
    "directory": "packages/holz-json-backend"
  },
  "exports": {
    ".": {
      "types": "./dist/holz-json-backend.d.ts",
      "require": "./dist/holz-json-backend.cjs",
      "import": "./dist/holz-json-backend.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Jesse Gibson",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "holz-backend",
    "ndjson",
    "structured",
    "logging"
  ],
  "scripts": {
    "prepack": "vite build",
    "test:unit": "vitest --color --passWithNoTests",
    "test:types": "tsc"
  },
  "devDependencies": {
    "@holz/core": "^0.8.2",
    "@types/node": "^24.0.0",
    "@vitest/coverage-v8": "^3.0.8",
    "typescript": "^5.8.2",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.3",
    "vite-tsconfig-paths": "^6.0.0",
    "vitest": "^3.0.8"
  }
}
