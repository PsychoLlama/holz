{
  "name": "@holz/console-backend",
  "version": "0.6.1",
  "description": "A console backend for Holz",
  "type": "module",
  "main": "./dist/holz-console-backend.cjs",
  "module": "./dist/holz-console-backend.js",
  "types": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/PsychoLlama/holz",
    "directory": "packages/holz-console-backend"
  },
  "exports": {
    ".": {
      "require": "./dist/holz-console-backend.cjs",
      "import": "./dist/holz-console-backend.js",
      "types": "./src/index.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Jesse Gibson",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "holz-backend",
    "console",
    "browser",
    "debugger"
  ],
  "scripts": {
    "prepack": "vite build",
    "test:unit": "vitest --color --passWithNoTests",
    "test:types": "tsc"
  },
  "peerDependencies": {
    "@holz/core": "workspace:*"
  },
  "devDependencies": {
    "@holz/core": "workspace:^0.6.0",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.0.0",
    "vitest": "^2.0.0"
  }
}
